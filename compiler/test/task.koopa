fun @fib(@n: i32): i32 {
%entry_fib:
	%fParam_n = alloc i32
	store @n, %fParam_n
	%0 = load %fParam_n
	%1 = lt %0, 2
	br %1, %then, %if_end
%then:
	%2 = load %fParam_n
	ret %2
%if_end:
	%3 = load %fParam_n
	%4 = sub %3, 1
	%5 = call @fib(%4)
	%6 = load %fParam_n
	%7 = sub %6, 2
	%8 = call @fib(%7)
	%9 = add %5, %8
	ret %9
}
fun @main(): i32 {
%entry_main:
	%10 = call @fib(20)
	ret %10
}
