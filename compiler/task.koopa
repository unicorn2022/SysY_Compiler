fun @main(): i32 {
%entry_main:
	@i = alloc i32
	store 0, @i
	jump %while_entry
%while_entry:
	%0 = load @i
	%1 = lt %0, 10
	br %1, %while_body, %while_end
%while_body:
	%2 = load @i
	%3 = eq %2, 5
	br %3, %then, %if_end
%then:
	jump %while_end
%if_end:
	%4 = load @i
	%5 = add %4, 1
	store %5, @i
	jump %while_entry
%while_end:
	%6 = load @i
	ret %6
}
